{
  "name": "basic-editor-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --filter='packages/*' --parallel dev & vite",
    "editor": "vite",
    "remotion": "npx remotion studio",
    "build": "pnpm -r --filter='packages/*' build && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@remotion-fast/core": "workspace:*",
    "@remotion-fast/ui": "workspace:*",
    "@remotion-fast/remotion-components": "workspace:*",
    "@remotion/player": "^4.0.363",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remotion": "^4.0.363"
  },
  "devDependencies": {
    "@remotion/cli": "^4.0.363",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^1.3.1"
  }
}
