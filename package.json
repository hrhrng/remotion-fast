{
  "name": "remotion-fast",
  "version": "0.1.0",
  "private": true,
  "description": "A powerful Remotion-based video editor component library",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "dev": "npm run dev --workspace=examples/basic-editor",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist examples/*/node_modules"
  },
  "keywords": [
    "remotion",
    "video-editor",
    "react",
    "component-library"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
