{
  "name": "remotion-fast",
  "version": "0.1.0",
  "private": true,
  "description": "A powerful Remotion-based video editor component library",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "dev": "npm run dev --workspace=examples/basic-editor",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist examples/*/node_modules"
  },
  "keywords": [
    "remotion",
    "video-editor",
    "react",
    "component-library"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@remotion/cli": "^4.0.363",
    "@remotion/player": "^4.0.363",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remotion": "^4.0.363",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  }
}
