{
  "name": "@remotion-fast/core",
  "version": "0.1.0",
  "description": "Core state management and types for Remotion Fast video editor",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "remotion",
    "video-editor",
    "state-management"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
