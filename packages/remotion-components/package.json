{
  "name": "@remotion-fast/remotion-components",
  "version": "0.1.0",
  "description": "Remotion rendering components for Remotion Fast video editor",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external remotion",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external remotion --watch"
  },
  "keywords": [
    "remotion",
    "video-rendering",
    "components"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "remotion": "^4.0.0"
  },
  "dependencies": {
    "@remotion-fast/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "remotion": "^4.0.363",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
